<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/lightdarkcolorgrey">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <!--            <ScrollView-->
            <!--                android:layout_width="match_parent"-->
            <!--                android:layout_height="match_parent"-->
            <!--                android:scrollbarStyle="outsideInset"-->
            <!--                app:layout_constraintStart_toStartOf="parent"-->
            <!--                app:layout_constraintTop_toTopOf="parent">-->

            <LinearLayout
                android:id="@+id/viewbuttonid"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:scrollbarStyle="outsideInset"
                android:scrollbars="vertical">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/activity_margin_10dp"
                    android:orientation="vertical"
                    app:cardCornerRadius="@dimen/activity_margin_10dp">

                    <TableLayout
                        android:id="@+id/step8Tab"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/bordergray"
                        android:scrollbars="horizontal|vertical"
                        android:stretchColumns="0"
                        android:visibility="visible">

                        <TableRow
                            android:id="@+id/colormyownvehicle"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@color/purple_500"
                            android:padding="@dimen/activity_margin_5dp"
                            android:weightSum="2">

                            <TextView
                                android:id="@+id/txtvehicletype"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/latinotypeantartidaroundedmedium"
                                android:padding="@dimen/activity_margin_5dp"
                                android:text="@string/type_b_commercial_vehicle"
                                android:textColor="@color/white"
                                android:textSize="@dimen/activity_margin_14dp"
                                android:textStyle="bold" />

                            <!--                            <ImageView-->
                            <!--                                android:id="@+id/statusbuttonid"-->
                            <!--                                android:layout_width="@dimen/activity_margin_25dp"-->
                            <!--                                android:layout_height="@dimen/activity_margin_25dp"-->
                            <!--                                android:layout_marginRight="@dimen/activity_margin_5dp" />-->

                            <!--                            <ImageView-->
                            <!--                                android:id="@+id/viewbuttonid"-->
                            <!--                                android:layout_width="@dimen/activity_margin_25dp"-->
                            <!--                                android:layout_height="@dimen/activity_margin_25dp"-->
                            <!--                                android:layout_marginLeft="@dimen/activity_margin_5dp"-->
                            <!--                                android:layout_marginRight="@dimen/activity_margin_15dp"-->
                            <!--                                app:srcCompat="@drawable/viewlist" />-->

                            <!--                            <ImageView-->
                            <!--                                android:id="@+id/deletebuttonid"-->
                            <!--                                android:layout_width="@dimen/activity_margin_30dp"-->
                            <!--                                android:layout_height="@dimen/activity_margin_30dp"-->
                            <!--                                android:layout_marginLeft="@dimen/activity_margin_5dp"-->
                            <!--                                android:layout_marginRight="@dimen/activity_margin_10dp"-->
                            <!--                                app:srcCompat="@drawable/icn_delete" />-->

                            <RelativeLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:id="@+id/deletebuttonid"
                                    android:layout_width="@dimen/activity_margin_25dp"
                                    android:layout_height="@dimen/activity_margin_25dp"
                                    android:layout_marginLeft="@dimen/activity_margin_5dp"
                                    android:layout_marginRight="@dimen/activity_margin_10dp"
                                    app:srcCompat="@drawable/icon_delete_white"
                                    tools:visibility="visible" />

                                <ImageView
                                    android:id="@+id/removebuttonid"
                                    android:layout_width="@dimen/activity_margin_25dp"
                                    android:layout_height="@dimen/activity_margin_25dp"
                                    android:layout_marginLeft="@dimen/activity_margin_10dp"
                                    android:layout_marginRight="@dimen/activity_margin_40dp"
                                    android:visibility="gone"
                                    app:srcCompat="@drawable/iconactive"
                                    tools:visibility="gone" />
                            </RelativeLayout>


                        </TableRow>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_margin_10dp"
                            android:layout_marginTop="@dimen/activity_margin_10dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="4.5">

                                <TextView
                                    android:id="@+id/textview17"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.4"
                                    android:fontFamily="@font/latinotypeantartidaroundedmedium"
                                    android:padding="@dimen/activity_margin_5dp"
                                    android:text="@string/registration_no2"
                                    android:textAlignment="textStart"
                                    android:textSize="@dimen/activity_margin_14dp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/colonregno"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="2.1"
                                    android:fontFamily="@font/latinotypeantartidaroundedmedium"
                                    android:padding="@dimen/activity_margin_5dp"
                                    android:text="@string/collen"
                                    android:textSize="@dimen/activity_margin_14dp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/txtregno"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/latinotypeantartidaroundedmedium"
                                    android:padding="@dimen/activity_margin_5dp"
                                    android:text="@string/kcy587e"
                                    android:textAlignment="textStart"
                                    android:textSize="@dimen/activity_margin_14dp"
                                    android:textStyle="bold" />


                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_margin_1dp"
                            android:layout_marginTop="@dimen/activity_margin_5dp"
                            android:orientation="vertical">


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/activity_margin_10dp"
                                android:layout_marginTop="@dimen/activity_margin_10dp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="4.5">

                                    <TextView
                                        android:id="@+id/textview26"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1.4"
                                        android:padding="@dimen/activity_margin_5dp"
                                        android:text="@string/certificate_no2"
                                        android:textAlignment="textStart"
                                        android:textSize="@dimen/activity_margin_14dp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/coloncertno"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="2.1"
                                        android:padding="@dimen/activity_margin_0dp"
                                        android:text="@string/collen"
                                        android:textAlignment="textStart"
                                        android:textSize="@dimen/activity_margin_14dp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/txtCertificateno"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:padding="@dimen/activity_margin_5dp"
                                        android:text="@string/a567125172"
                                        android:textAlignment="textStart"
                                        android:textSize="@dimen/activity_margin_14dp"
                                        android:textStyle="bold" />


                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/activity_margin_10dp"
                                android:layout_marginTop="@dimen/activity_margin_10dp"

                                android:layout_marginBottom="@dimen/activity_margin_10dp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="horizontal"
                                    android:weightSum="4.5">

                                    <TextView
                                        android:id="@+id/textview37"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1.4"
                                        android:padding="@dimen/activity_margin_5dp"
                                        android:text="@string/insurance_company"
                                        android:textAlignment="textStart"
                                        android:textSize="@dimen/activity_margin_14dp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/coloncmpnyname"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="2.1"

                                        android:text="@string/collen"
                                        android:textAlignment="textStart"
                                        android:textSize="@dimen/activity_margin_14dp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/txtInsucmpny"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:text="@string/apa_insurance_company_ltd"
                                        android:textAlignment="textStart"
                                        android:textSize="@dimen/activity_margin_14dp"
                                        android:textStyle="bold" />


                                </LinearLayout>
                            </LinearLayout>

                        </LinearLayout>
                    </TableLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
            <!--            </ScrollView>-->
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
